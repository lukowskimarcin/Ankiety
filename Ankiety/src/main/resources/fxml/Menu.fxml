<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlers.MenuControler">
  <menus>
    <Menu mnemonicParsing="false" text="%mFile">
      <items>
        <MenuItem fx:id="mClose" mnemonicParsing="false" onAction="#onClose" text="%mClose" />
      </items>
    </Menu>
      <Menu mnemonicParsing="false" text="%mHelp">
         <items>
            <MenuItem fx:id="mAbout" mnemonicParsing="false" onAction="#onAbout" text="%mAbout" />
         </items></Menu>
  </menus>
</MenuBar>
